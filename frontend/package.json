{
  "private": true,
  "name": "frontend",
  "license": "MIT",
  "author": "bkiac <benceknab.iac@gmail.com>",
  "version": "0.0.1",
  "scripts": {
    "pre-commit": "lint-staged",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "yarn prettier && yarn eslint",
    "lint": "eslint src/ --ext .js,.ts,.tsx --cache --fix",
    "test": "yarn test:react && yarn test:webapi",
    "test:react": "react-scripts test --watchAll=false",
    "test:react:watch": "react-scripts watch",
    "test:webapi": "jest -c jest.config.js",
    "prettier": "prettier --write \"src/**/*\" && prettier-package-json --write",
    "type": "tsc --noEmit",
    "watch": "react-scripts start"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "axios": "^0.19.2",
    "base64-js": "^1.3.1",
    "date-fns": "^2.12.0",
    "memoize-one": "^5.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/base64-js": "^1.2.5",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/puppeteer": "^2.0.1",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "jest": "^25.4.0",
    "jest-puppeteer": "^4.4.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "puppeteer": "^3.0.1",
    "ts-jest": "^25.4.0"
  }
}
